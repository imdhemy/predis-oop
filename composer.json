{
    "name": "imdhemy/predis-oop",
    "description": "An object oriented feature-complete redis client for php",
    "type": "library",
    "require": {
        "predis/predis": "^1.1",
        "illuminate/support": "^8.18"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/var-dumper": "^5.2",
        "friendsofphp/php-cs-fixer": "^2.17",
        "vimeo/psalm": "^4.3"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "imdhemy",
            "email": "imdhemy@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Imdhemy\\Redis\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    }
}
